@model DalaWeb.Domain.Entities.Counters.CounterValues

@{
    ViewBag.Title = "Показания счетчиков";
}

<h2>Создание</h2>

@using (Html.BeginForm("UploadFile", "CounterValues", FormMethod.Post, new { enctype = "multipart/form-data" }))
{     
    @Html.AntiForgeryToken()
    <input type="file" name="files" accept=".xlsx"><br>
    @Html.DropDownList("ServiceId", (SelectList)ViewBag.ServiceId)
    <br />
    <input type="submit" value="Загрузить файл">
}
<div>
    @Html.ActionLink("Вернуться", "Index")
</div>

