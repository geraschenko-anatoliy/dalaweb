@model IEnumerable<DalaWeb.Domain.Entities.PDFStorages.PDFAbonentMonthlyReceipt>

@{
    ViewBag.Title = "Гостевой доступ";
    Layout = "~/Views/Shared/_GuestLayout.cshtml";
}

<div class="span12">
@using (Html.BeginForm("PDFReceiptsForAbonentList", "Guest"))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <div class="span4 offset4 pagination-centered">
        <fieldset>
            <!-- Form Name -->
            <legend>Просмотр квитанций</legend>
            <!-- Password input-->
            <div class="control-group">
                <div class="editor-label">
                    @Html.Label("Введите ИНН")
                </div>
                <div class="editor-field">
                    @Html.Editor("INN", "1234567890")
                </div>
            </div>
            <div class="control-group">
                <input type="submit" class="btn btn-success" value="ОК" />
            </div>
        </fieldset>
    </div>
}
</div>

<div class="span12"></div>

